  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.38.0",
    "eslint-plugin-vue": "^9.10.0",
    "gulp": "^4.0.2",
    "gulp-minify-css": "^1.2.4",
    "gulp-sass": "^5.1.0",
    "rain-light-ui": "^0.0.12",
    "sass": "^1.61.0",
    "typescript": "^4.9.3",
    "unplugin-vue-define-options": "^1.3.3",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.2.0",
    "vue-tsc": "^1.2.0"
  }
