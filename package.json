{
  "name": "rain-light-ui",
  "version": "0.0.13",
  "type": "module",
  "description": "组件库描述",
  "main": "./dist/rain-light-ui.umd.js",
  "module": "./dist/rain-light-ui.js",
  "types": "./dist/rain-light-ui.d.ts",
  "exports": {
    ".": {
      "import": "./dist/rain-light-ui.js",
      "require": "./dist/rain-light-ui.umd.js"
    },
